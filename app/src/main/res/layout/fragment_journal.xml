<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:context=".JournalFragment">
        <TextView
            android:id="@+id/Guide"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="CANCELORS"
            android:textSize="50dp"
            app:layout_constraintStart_toStartOf="@+id/guideline6"
            app:layout_constraintEnd_toStartOf="@id/guideline5"
            android:textAllCaps="true"
            app:layout_constraintTop_toTopOf="parent"
            />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="59dp"
            android:layout_height="100dp"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/guideline6"
            app:srcCompat="@drawable/ic_person_black_24dp"
            tools:ignore="VectorDrawableCompat"
            app:layout_constraintTop_toBottomOf="@id/Guide"/>
        <Button
            android:id="@+id/button8"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:text="DAN KIPLAGAT"

            app:layout_constraintEnd_toStartOf="@+id/guideline5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="59dp"
            android:layout_height="100dp"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/guideline6"
            app:srcCompat="@drawable/ic_person_black_24dp"
            tools:ignore="VectorDrawableCompat"
            app:layout_constraintTop_toBottomOf="@id/button8"/>
        <Button
            android:id="@+id/button88"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:text="GRACE OCHOLA"
            app:layout_constraintEnd_toStartOf="@+id/guideline5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView22"
            app:layout_constraintBottom_toBottomOf="@+id/imageView22"
            app:layout_constraintTop_toTopOf="@+id/imageView22" />

        <ImageView
            android:id="@+id/image"
            android:layout_width="59dp"
            android:layout_height="100dp"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/guideline6"
            app:srcCompat="@drawable/ic_person_black_24dp"
            tools:ignore="VectorDrawableCompat"
            app:layout_constraintTop_toBottomOf="@id/button88"/>
        <Button
            android:id="@+id/butto"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:text="EDUIN ONGANGA"
            app:layout_constraintEnd_toStartOf="@+id/guideline5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/image"
            app:layout_constraintBottom_toBottomOf="@+id/image"
            app:layout_constraintTop_toTopOf="@+id/image" />

        <ImageView
            android:id="@+id/imageV"
            android:layout_width="59dp"
            android:layout_height="100dp"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="@+id/guideline6"
            app:srcCompat="@drawable/ic_person_black_24dp"
            tools:ignore="VectorDrawableCompat"
            app:layout_constraintTop_toBottomOf="@id/butto"/>
        <Button
            android:id="@+id/button"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:text="IVY BARCHOCK"
            app:layout_constraintEnd_toStartOf="@+id/guideline5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageV"
            app:layout_constraintBottom_toBottomOf="@+id/imageV"
            app:layout_constraintTop_toTopOf="@+id/imageV" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="16dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="16dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>